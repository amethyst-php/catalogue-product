{
    "name": "amethyst/catalogue-product",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-catalogue-product", "catalogue-product"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*",
        "amethyst/catalogue": "0.2.*",
        "amethyst/product": "0.2.*",
        "amethyst/price": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "catalogue-product",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\CatalogueProductServiceProvider"
            ]
        }
    }
}
